<?xml version="1.0" encoding="UTF-8"?>
<dbfound xmlns="http://dbfound.googlecode.com/conf" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dbfound.googlecode.com/conf https://raw.githubusercontent.com/nfwork/dbfound/master/tags/conf.xsd">

	<system>
		<modelModifyCheck>true</modelModifyCheck>
		<camelCaseToUnderscore>true</camelCaseToUnderscore>
	</system>

	<database>
		<dataSourceConnectionProvide className="org.apache.commons.dbcp.BasicDataSource" dialect="MySqlDialect">
			<properties>
				<property name="driverClassName" value="com.mysql.jdbc.Driver" />
				<property name="url" value="jdbc:mysql://smysql01.sz.gomo.com/dbfound?useUnicode=true&amp;characterEncoding=utf8" />
				<property name="username" value="jbjava" />
				<property name="password" value="jb98" />
				<property name="initialSize" value="5" />
				<property name="maxActive" value="10" />
				<property name="maxIdle" value="5" />
				<property name="testOnBorrow" value="true" />
				<property name="validationQuery" value="select 1" />
			</properties>
		</dataSourceConnectionProvide>
	</database>

	<web>
		<interceptor>com.nfwork.demo.check.AccessCheckInterceptor</interceptor>
	</web>

</dbfound>
